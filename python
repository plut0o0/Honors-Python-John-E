30.

oj= float(2*3.48)
milk= float(3*1.89)
ttl= float(milk + oj)
print ("2 gallons of orange juice = $" + str(oj) + "\n3 quarts of milk = $" + str(milk) + "\nTotal Cost = $" + str(ttl))

31.

length=15
width=8
area = length*width
perimeter = length+length+width+width
a = "***********************"
b = "*                     *"
print ("        15cm")
print (a)
print (b)
print (b)
print (str(b) + "   8cm")
print (b)
print (b)
print (b)
print (a)
print (" The area of the above rectangle is " + str(area) + "cm, and the perimeter is " + str(perimeter) + "cm.")

32.
f1a=(5*5)-3
f1b=(5**2*2-5*5+6)
f2a=(5*10)-3
f2b=(10**2*2-5*10+6)
print ("X\t5x-3\t2x^2-5x+6")
print ("5\t" + str(f1a) + "\t\t" + str(f1b))
print ("10\t" + str(f2a) + "\t\t" + str(f2b))


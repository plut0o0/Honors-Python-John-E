36.

add = 10+3
sub = 10-3
mult = 10*3
div = 10/3
pwr = 10**3
divwhole = 10//3
print ("10+3=" + str(add))
print ("10-3=" + str(sub))
print ("10*3=" + str(mult))
print ("10/3=" + str(div))
print ("10**3=" + str(pwr))
print ("10//3=" + str(divwhole))

